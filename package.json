{
  "name": "cass",
  "version": "1.4.0",
  "description": "Competency and Skills System",
  "main": "src/main/server.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "npm run test:deps & npm-run-all --parallel -r test:nodemon test:test",
    "test:deps": "docker-compose up -d elasticsearch",
    "test:nodemon": "nodemon src/main/server.js",
    "test:test": "wait-on http://localhost/api/ping & node node_modules/cassproject/test.js",
    "dev": "npm-run-all --parallel test:deps test:nodemon",
    "run": "pm2 start ./pm2.config.js && pm2 logs",
    "logs": "pm2 logs",
    "stop": "pm2 stop all",
    "docs": "yuidoc -c yuidoc.json -o docs -e .js node_modules/cassproject/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cassproject/CASS.git"
  },
  "keywords": [
    "cass",
    "competency",
    "skills",
    "cassproject"
  ],
  "author": "Eduworks Corporation <cass@eduworks.com> (http://cassproject.org)",
  "contributors": [
    "Ronald 'Fritz' Ray <fritz.ray@eduworks.com>",
