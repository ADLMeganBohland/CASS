process=process.forEach(
	paramName="key",
	valueName="val",
	op=#object(
		measureType="http://schema.eduworks.com/cass/assertionProcessing/quadnary",
		level="@val",
		confidence=#toDouble(obj="1.0")
	)
).displayJson(_collapse="true");

